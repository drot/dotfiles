! Font hinting settings
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! Cursor
Xcursor.theme: Adwaita

! Emacs
Emacs.font: fontset-startup
Emacs.Fontset-0: -BE5N-Iosevka Fixed SS05-normal-normal-normal-*-16-*-*-*-m-0-fontset-startup,\
    arabic:-GOOG-Noto Sans Arabic-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    burmese:-GOOG-Noto Sans Myanmar-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    ethiopic:-GOOG-Noto Sans Ethiopic-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    gujarati:-GOOG-Noto Sans Gujarati-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    hangul:-GOOG-Noto Sans CJK KR-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    han:-GOOG-Noto Sans CJK TC-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    hebrew:-GOOG-Noto Sans Hebrew-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    kana:-GOOG-Noto Sans CJK JP-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1,\
    symbol:-GOOG-Noto Color Emoji-normal-normal-normal-*-*-*-*-*-m-0-iso10646-1,\
    thai:-GOOG-Noto Sans Thai-normal-normal-normal-*-16-*-*-*-*-0-iso10646-1
Emacs.background: #222222
Emacs.menuBar: off
Emacs.toolBar: off
Emacs.verticalScrollBars: off
Emacs.cursorBlink: off

! XTerm
XTerm*locale: true
XTerm*termName: xterm-256color
XTerm*bellIsUrgent: true
XTerm*saveLines: 65535
XTerm*borderWidth: 0

! Release Meta binding
XTerm*metaSendsEscape: true
XTerm*eightBitInput: false

! Scrolling
XTerm*ScrollBar: false
XTerm*ScrollKey: true

! Dabbrev expand
XTerm*VT100.Translations: #override \n\ Meta <KeyPress> /:dabbrev-expand()

! Font
XTerm*faceName: Iosevka Fixed SS05:pixelsize=16

! Main colors
XTerm*foreground: #c5c8c6
XTerm*background: #1d1f21
XTerm*cursorColor: #c5c8c6

! Stylized colors
XTerm*colorIT: #b5bd68
XTerm*colorBD: #c5c8c6
XTerm*colorUL: #81a2be

! Colors
XTerm*color0: #1d1f21
XTerm*color1: #cc6666
XTerm*color2: #b5bd68
XTerm*color3: #f0c674
XTerm*color4: #81a2be
XTerm*color5: #b294bb
XTerm*color6: #8abeb7
XTerm*color7: #c5c8c6
XTerm*color8: #969896
XTerm*color9: #cc6666
XTerm*color10: #b5bd68
XTerm*color11: #f0c674
XTerm*color12: #81a2be
XTerm*color13: #b294bb
XTerm*color14: #8abeb7
XTerm*color15: #ffffff

! XMessage
Xmessage*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-iso10646-*
Xmessage*background: red
Xmessage*foreground: yellow
Xmessage*form.*.shapeStyle: rectangle
Xmessage*Scrollbar.width: 1
Xmessage*Scrollbar.borderWidth: 0
Xmessage*Buttons: OK
Xmessage*defaultButton: OK
