! drot X resources

! Cursor
Xcursor.theme: Neutral

! Emacs
Emacs.font: Terminus-10

! URXVT
! General
URxvt.termName: rxvt-unicode-256color
URxvt.loginShell: false
URxvt.scrollBar: false
URxvt.saveLines: 65535
URxvt.cursorBlink: false
URxvt.urgentOnBell: true
URxvt.underlineURLs: true
URxvt.colorUL: #AF5FD7
URxvt.cursorColor: #E25454
URxvt.secondaryScroll: true
URxvt.internalBorder: 0

! Extensions
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/conkeror
URxvt.matcher.button: 1
URxvt.cutchars: '"'()*<>[]{|}

! Appearance
URxvt.font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
URxvt.boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! Erebus color theme
URxvt.background: #261515
URxvt.foreground: #FFFFFF

! black + red
URxvt.color0: #757575
URxvt.color1: #FF5F5F

! green + yellow
URxvt.color2: #DE8A36
URxvt.color3: #D78787

! blue + purple
URxvt.color4: #AF5FD7
URxvt.color5: #FF87D7

! cyan + white
URxvt.color6: #DEA3E5
URxvt.color7: #B8B8B8

! bright-black + bright-red
URxvt.color8: #B8B8B8
URxvt.color9: #D78787

! bright-green + bright-yellow
URxvt.color10: #FF9F6F
URxvt.color11: #FF5F5F

! bright-blue + bright-purple
URxvt.color12: #DEA3E5
URxvt.color13: #D7AFAF

! bright-cyan + bright-white
URxvt.color14: #AF5FD7
URxvt.color15: #757575

! XTERM
! General
xterm*internalBorder: 0
xterm*saveLines: 65535
xterm*dynamicColors: on
xterm*highlightSelection: true
xterm*utf8: 2
xterm*eightBitInput: true

! Appearance
xterm*font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
xterm*boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! Erebus color theme
xterm*background: #261515
xterm*foreground: #FFFFFF

! black + red
xterm*color0: #757575
xterm*color1: #FF5F5F

! green + yellow
xterm*color2: #DE8A36
xterm*color3: #D78787

! blue + purple
xterm*color4: #AF5FD7
xterm*color5: #FF87D7

! cyan + white
xterm*color6: #DEA3E5
xterm*color7: #B8B8B8

! bright-black + bright-red
xterm*color8: #B8B8B8
xterm*color9: #D78787

! bright-green + bright-yellow
xterm*color10: #FF9F6F
xterm*color11: #FF5F5F

! bright-blue + bright-purple
xterm*color12: #DEA3E5
xterm*color13: #D7AFAF

! bright-cyan + bright-white
xterm*color14: #AF5FD7
xterm*color15: #757575

