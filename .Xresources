! Cursor
Xcursor.theme: Neutral

! Emacs
Emacs.font: Terminus-10

! URXVT
! General
URxvt.termName: rxvt-unicode-256color
URxvt.scrollBar: false
URxvt.saveLines: 80000
URxvt.urgentOnBell: true
URxvt.underlineURLs: true
URxvt.colorUL: #AF5FD7
URxvt.cursorColor: #E25454
URxvt.internalBorder: 0

! Extensions
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/conkeror
URxvt.matcher.button: 1
URxvt.cutchars: '"'()*<>[]{|}

! Font
URxvt.font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
URxvt.boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! Erebus color theme
URxvt.background: #261515
URxvt.foreground: #FFFFFF

URxvt.color0: #757575
URxvt.color1: #FF5F5F
URxvt.color2: #DE8A36
URxvt.color3: #D78787
URxvt.color4: #AF5FD7
URxvt.color5: #FF87D7
URxvt.color6: #DEA3E5
URxvt.color7: #B8B8B8
URxvt.color8: #B8B8B8
URxvt.color9: #D78787
URxvt.color10: #FF9F6F
URxvt.color11: #FF5F5F
URxvt.color12: #DEA3E5
URxvt.color13: #D7AFAF
URxvt.color14: #AF5FD7
URxvt.color15: #757575

! XTERM
! General
XTerm*termName: xterm-256color
XTerm*locale: true
XTerm*metaSendsEscape: true
XTerm*saveLines: 80000
XTerm*fastScroll: true
XTerm*highlightSelection: true
XTerm*cursorColor: #E25454

! Font
XTerm*font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
XTerm*boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! Erebus color theme
XTerm*background: #261515
