! drot X resources

! Cursor
Xcursor.theme: Neutral

! Emacs
Emacs.font: Terminus-10

! URXVT
! General
URxvt.termName: rxvt-unicode-256color
URxvt.loginShell: false
URxvt.scrollBar: false
URxvt.saveLines: 65535
URxvt.cursorBlink: false
URxvt.urgentOnBell: true
URxvt.underlineURLs: true
URxvt.colorUL: #AF5FD7
URxvt.cursorColor: #E25454
URxvt.secondaryScroll: true
URxvt.internalBorder: 0

! Extensions
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/conkeror
URxvt.matcher.button: 1
URxvt.cutchars: '"'()*<>[]{|}

! Appearance
URxvt.font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
URxvt.boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! X::DotShare color theme
URxvt.foreground: #D7D0C7
URxvt.background: #151515

!black
URxvt.color0: #101010
URxvt.color8: #404040

!red
URxvt.color1: #E84F4F
URxvt.color9: #D23D3D

!green
URxvt.color2: #B8D68C
URxvt.color10:#A0CF5D

!yellow
URxvt.color3: #E1AA5D
URxvt.color11: #F39D21

!blue
URxvt.color4: #7DC1CF
URxvt.color12: #4E9FB1

!magenta
URxvt.color5: #9B64FB
URxvt.color13: #8542FF

!cyan
URxvt.color6: #6D878D
URxvt.color14: #42717B

!white
URxvt.color7: #dddddd
URxvt.color15: #dddddd

! XTERM
! General
xterm*internalBorder: 0
xterm*saveLines: 65535
xterm*dynamicColors: on
xterm*highlightSelection: true
xterm*utf8: 2
xterm*eightBitInput: true

! Appearance
xterm*font: -xos4-terminus-medium-*-*-*-14-*-*-*-*-*-iso8859-2
xterm*boldFont: -xos4-terminus-bold-*-*-*-14-*-*-*-*-*-iso8859-2

! X::DotShare color theme
xterm*foreground: #D7D0C7
xterm*background: #151515

!black
xterm*color0: #101010
xterm*color8: #404040

!red
xterm*color1: #E84F4F
xterm*color9: #D23D3D

!green
xterm*color2: #B8D68C
xterm*color10:#A0CF5D

!yellow
xterm*color3: #E1AA5D
xterm*color11: #F39D21

!blue
xterm*color4: #7DC1CF
xterm*color12: #4E9FB1

!magenta
xterm*color5: #9B64FB
xterm*color13: #8542FF

!cyan
xterm*color6: #6D878D
xterm*color14: #42717B

!white
xterm*color7: #dddddd
xterm*color15: #dddddd

