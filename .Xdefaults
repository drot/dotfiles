! drot X resources

! Emacs
Emacs.font: -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso10646-1

! URXVT
! General
URxvt.termName: rxvt-256color
URxvt.loginShell: false
URxvt.scrollBar: false
URxvt.saveLines: 65535
URxvt.cursorBlink: true
URxvt.urgentOnBell: true
URxvt.underlineURLs: true
URxvt.colorUL: #dfaf8f
URxvt.cursorColor: #dcdccc
URxvt.internalBorder: 1
URxvt.secondaryScroll: true

! Extensions
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: /usr/bin/conkeror
URxvt.matcher.button: 1
URxvt.cutchars: '"'()*<>[]{|}

! Appearance
URxvt.font: -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-iso10646-1
URxvt.boldFont: -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-iso10646-1

! Zenburn color-scheme
URxvt.background:  #3f3f3f
URxvt.foreground:  #dcdccc

! black + red
URxvt.color0:      #1E2320
URxvt.color1:      #705050

! green + yellow
URxvt.color2:      #60b48a
URxvt.color3:      #dfaf8f

! blue + purple
URxvt.color4:      #506070
URxvt.color5:      #dc8cc3

! cyan + white
URxvt.color6:      #8cd0d3
URxvt.color7:      #dcdccc

! bright-black + bright-red
URxvt.color8:      #709080
URxvt.color9:      #dca3a3

! bright-green + bright-yellow
URxvt.color10:     #c3bf9f
URxvt.color11:     #f0dfaf

! bright-blue + bright-purple
URxvt.color12:     #94bff3
URxvt.color13:     #ec93d3

! bright-cyan + bright-white
URxvt.color14:     #93e0e3
URxvt.color15:     #ffffff
