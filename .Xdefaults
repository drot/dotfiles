! drot's X application resource database


! X Cursor
Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 22

! Emacs
Emacs.font: -*-proggytinysz-medium-*-*-*-*-*-*-*-*-*-*-*
Emacs.menuBar: off
Emacs.toolBar: -1
Emacs.verticalScrollBars: off

! URXVT
!
! General
urxvt*termName: rxvt-256color
urxvt*loginShell: true
urxvt*scrollBar: false
urxvt*saveLines: 65535
urxvt*cursorBlink: true
urxvt*urgentOnBell: true
urxvt*underlineURLs: true
urxvt*colorUL: #86a2be
urxvt*cursorColor: #DCDCCC
!
! Extensions
urxvt*perl-lib: /usr/lib/urxvt/perl/
urxvt*perl-ext-common: default,matcher
urxvt*urlLauncher: /usr/bin/chromium
urxvt*matcher.button: 1
! So we catch even nasty URLs
urxvt*cutchars: `()'*<>[]{|}"
!
! Appearance
urxvt*font: -*-proggytinysz-medium-*-*-*-*-*-*-*-*-*-*-*
urxvt*boldFont: -*-proggytinysz-medium-*-*-*-*-*-*-*-*-*-*-*

! Color theme
urxvt*background:       #222222
urxvt*foreground:       #e1e2db

urxvt*color0:        	#303030
urxvt*color1:        	#c03000
urxvt*color2:        	#b1d631
urxvt*color3:        	#fecf35
urxvt*color4:        	#426870
urxvt*color5:        	#6d506d
urxvt*color6:        	#4bb5c1
urxvt*color7:        	#e2e2e5
urxvt*color8:        	#5f5f5f
urxvt*color9:        	#ff3a78
urxvt*color10:		#a6cd07
urxvt*color11:       	#b5c865
