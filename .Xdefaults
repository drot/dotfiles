! drot's X application resource database


! X Cursor
Xcursor.theme: Vanilla-DMZ-AA
Xcursor.size: 22

! URXVT
!
! General
URxvt*termName: rxvt-256color
URxvt*loginShell: true
URxvt*scrollBar: false
URxvt*saveLines: 65535
URxvt*cursorBlink: true
URxvt*urgentOnBell: true
URxvt*underlineURLs: true
URxvt*colorUL: #86a2be
URxvt*depth: 32
!
! Extensions
URxvt*perl-lib: /usr/lib/URxvt/perl/
URxvt*perl-ext-common: default,matcher
URxvt*urlLauncher: /usr/bin/chromium
URxvt*matcher.button: 1
! So we catch even nasty URLs
URxvt*cutchars: `()'*<>[]{|}"
!
! Appearance
URxvt*font: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt*boldFont: -*-terminus-medium-*-*-*-12-*-*-*-*-*-*-*

! tangoesque scheme
URxvt*background: rgba:0000/0000/0000/bbbb
URxvt*foreground: #aaaaaa
! Black
*color0:      #000000
*color8:      #555753
! Red
*color1:      #ff6565
*color9:      #ff8d8d
! Green
*color2:      #93d44f
*color10:     #c8e7a8
! Yellow
*color3:      #eab93d
*color11:     #ffc123
! Blue
*color4:      #204a87
*color12:     #3465a4
! Mangenta
*color5:      #ce5c00
*color13:     #f57900
! Cyan 
*color6:      #89b6e2
*color14:     #46a4ff
! White
*color7:      #cccccc
*color15:     #ffffff
